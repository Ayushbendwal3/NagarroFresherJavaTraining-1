#logoutBtn,#downloadBtn,#submitBtn,#resetBtn{
float:right ;
}
#table,th,.td {
	border-collapse: collapse;
	border: 1px solid black ;
	text-align: center;
}
table{width:90%;}

.editLink{
	color: blue;
    text-decoration: underline;
    cursor: pointer;
}
#editSec{
	position:fixed ;
	display:none ;
	top:40% ;
	left:35% ;
	width:350px ;
	z-index:2 ;
}
#overlay{
	position:fixed ;
	display:none ;
	width:100% ;
	height:100%  ;
	background-color:darkgray ;
	opacity:0.9 ;
	z-index:1;
}